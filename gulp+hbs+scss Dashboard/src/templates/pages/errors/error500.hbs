{{#extend "layouts/dashboard/simple" title="pages-error500"  active="pages-error500" path="../" }}
{{#content "content"}}
<div class="bg-primary-subtle vh-100 position-relative">
    <div class="container text-center">
        <div class="row flex-column justify-content-center align-items-center vh-100">
            <div style="z-index:1">
                <img src="{{path}}assets/images/error/500.png" class="img-fluid mb-4 w-50" alt="500" loading="lazy">
                <p class="mt-5 text-body">The Page You Requested Could Not Be Found<br> Please Go Back To Homepage</p>
                <a class="mt-3 btn bg-primary text-white text-center" href="../index.html">Back to Home</a>
            </div>
        </div>
    </div>
    <div>
        <img src="{{path}}assets/images/error/error-bg-4.png" class="position-absolute top-0 start-0" alt="image">
        <img src="{{path}}assets/images/error/error-bg-2.png" class="position-absolute error-bg-one" alt="image">
        <img src="{{path}}assets/images/error/error-bg-3.png" class="position-absolute error-bg-two" alt="image">
        <img src="{{path}}assets/images/error/error-bg-1.png" class="position-absolute bottom-0 end-0" alt="image">
    </div>
</div>
{{/content}}
{{/extend}}
