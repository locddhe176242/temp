<!-- Google Font Api KEY-->
<meta name="google_font_api" content="{{ googleFontApi }}">
<!-- Favicon -->
<link rel="shortcut icon" href="{{ path }}assets/images/favicon.ico" />

<!-- Library / Plugin Css Build -->
<link rel="stylesheet" href="{{ path }}assets/css/core/libs.min.css" />

<!-- flaticon css -->
<link rel="stylesheet" href="{{ path }}assets/vendor/flaticon/css/flaticon.css" />

<!-- font-awesome css -->
<link rel="stylesheet" href="{{ path }}assets/vendor/font-awesome/css/all.min.css" />

{{#compare isFlatpickr '==' 'true'}}
<!-- Flatpickr css -->
<link rel="stylesheet" href="{{ path }}assets/vendor/flatpickr/dist/flatpickr.min.css" />
{{/compare}}

{{#compare isSweetalert '==' 'true'}}
<!-- Sweetlaert2 css -->
<link rel="stylesheet" href="{{ path }}assets/vendor/sweetalert2/dist/sweetalert2.min.css"/>
{{/compare}}

{{#compare isNoUISlider '==' 'true'}}
<!-- NoUI Slider css -->
<link rel="stylesheet" href="{{path}}assets/vendor/noUiSilder/style/nouislider.min.css">
{{/compare}}

{{#compare isSwiperSlider '==' 'true'}}
<!-- SwiperSlider css -->
<link rel="stylesheet" href="{{path}}assets/vendor/swiperSlider/swiper-bundle.min.css">
{{/compare}}

<!-- Kivicare Design System Css -->
<link rel="stylesheet" href="{{ path }}assets/css/kivicare.min.css?v={{version}}" />

<!-- Custom Css -->
<link rel="stylesheet" href="{{ path }}assets/css/custom.min.css?v={{version}}" />

<!-- Rtl Css -->
<link rel="stylesheet" href="{{ path }}assets/css/rtl.min.css?v={{version}}" />

<!-- Customizer Css -->
<link rel="stylesheet" href="{{ path }}assets/css/customizer.min.css?v={{version}}"/>

<!-- Google Font -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Heebo:wght@300;400;500;600;700&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;1,300;1,400;1,500&display=swap" rel="stylesheet">


