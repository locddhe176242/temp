<div class="section-padding">
   <div class="container">
      <div class="row">
         <div class="col-xl-8">
            <div class="iq-blog blog-detail position-relative border">
               {{#compare isImg '==' 'true'}}
               <a href="blog-details.html"
                  class="blog-image d-block overflow-hidden">
                  <img src="{{path}}assets/images/{{blog-image}}"
                     alt="blog-image"
                     class="img-fluid w-100" loading="lazy">
               </a>
               {{/compare}}
               {{#compare isVideo '==' 'true'}}
               <div>
                  <iframe class="elementor-video" allowfullscreen
                     allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
                     title="Iqonic Design - User Reviews | Very Professional Work | Iqonic Design"
                     width="500"
                     height="400"
                     src="https://www.youtube.com/embed/VeDdpy4CdeM?controls=1&amp;rel=0&amp;playsinline=0&amp;modestbranding=0&amp;autoplay=0&amp;enablejsapi=1&amp;origin=https%3A%2F%2Fwordpress.iqonic.design&amp;widgetid=1"
                     id="widget2"></iframe>
               </div>
               {{/compare}}
               {{#compare isAudio '==' 'true'}}
               <div>
                  <iframe
                     src="https://w.soundcloud.com/player/?visual=false&amp;url=https%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F417751212&amp;show_artwork=true&amp;maxheight=750&amp;maxwidth=720&amp;auto_play=false&amp;buying=true&amp;liking=true&amp;download=true&amp;sharing=true&amp;show_comments=true&amp;show_playcount=true&amp;show_user=true&amp;color"
                     width="500"></iframe>
               </div>
               {{/compare}}
               {{#compare isGallery '==' 'true'}}
               <div class="row row-cols-xl-3 row-cols-md-2 row-cols-1">
                  <div class="col p-0">
                     <a data-fslightbox="sidebar-gallery"
                        href="{{path}}assets/images/gallery/blog-1.jpg">
                        <img src="{{path}}assets/images/gallery/blog-1.jpg"
                           class="p-2 pt-lg-0 pt-md-0 img-fluid w-100"
                           alt="profile-image" loading="lazy">
                     </a>
                  </div>
                  <div class="col p-0">
                     <a data-fslightbox="sidebar-gallery"
                        href="{{path}}assets/images/gallery/blog-2.jpg">
                        <img src="{{path}}assets/images/gallery/blog-2.jpg"
                           class="p-2 pt-lg-0 pt-md-0 img-fluid w-100"
                           alt="profile-image" loading="lazy">
                     </a>
                  </div>
                  <div class="col p-0">
                     <a data-fslightbox="sidebar-gallery"
                        href="{{path}}assets/images/gallery/blog-3.jpg">
                        <img src="{{path}}assets/images/gallery/blog-3.jpg"
                           class="p-2  pt-lg-0 pt-md-2 img-fluid w-100"
                           alt="profile-image" loading="lazy">
                     </a>
                  </div>
                  <div class="col p-0">
                     <a data-fslightbox="sidebar-gallery"
                        href="{{path}}assets/images/gallery/blog-4.jpg">
                        <img src="{{path}}assets/images/gallery/blog-4.jpg"
                           class="p-2 img-fluid w-100"
                           alt="profile-image" loading="lazy">
                     </a>
                  </div>
                  <div class="col p-0">
                     <a data-fslightbox="sidebar-gallery"
                        href="{{path}}assets/images/gallery/blog-5.jpg">
                        <img src="{{path}}assets/images/gallery/blog-5.jpg"
                           class="p-2 img-fluid w-100"
                           alt="profile-image" loading="lazy">
                     </a>
                  </div>
                  <div class="col p-0">
                     <a data-fslightbox="sidebar-gallery"
                        href="{{path}}assets/images/gallery/blog-6.jpg">
                        <img src="{{path}}assets/images/gallery/blog-6.jpg"
                           class="p-2 img-fluid w-100"
                           alt="profile-image" loading="lazy">
                     </a>
                  </div>
               </div>
               {{/compare}}
               {{#compare isLink '==' 'true'}}
               <div class="px-3 pt-4 bg-white">
                  <a href="https://iqonic.design/"><i
                        class="fas fa-link text-primary"></i></a>
               </div>
               {{/compare}}
               <div class="iq-post-details bg-white p-4">
                  <div class="iq-blog-meta">
                     <ul class="list-inline">
                        <li
                           class="position-relative list-inline-item text-uppercase">
                           <a href="{{path}}blog-author.html">
                              <i class="far fa-user me-2 text-primary"
                                 aria-hidden="true"></i>
                              <span>{{blog-author}}</span>
                           </a>
                        </li>
                        <li
                           class="position-relative list-inline-item text-uppercase">
                           <a href="{{path}}blog-date.html">
                              <i class="far fa-calendar-alt me-2 text-primary"
                                 aria-hidden="true"></i>
                              <span>{{blog-publish-date}}</span>
                           </a>
                        </li>
                        <li
                           class="position-relative list-inline-item text-uppercase">
                           <span class="iq-blog-cat">
                              <a href="{{path}}blog-category.html">
                                 {{blog-category}}
                              </a>
                           </span>
                        </li>
                     </ul>
