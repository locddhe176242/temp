                  {{#compare isTag '==' 'true'}}
                  <ul class="blogtag list-inline mt-5 mb-0">
                     <li class="label d-inline me-2">Tags:</li>
                     <li class="d-inline">
                        <a href="#" class="position-relative">{{blog-tag}}</a>
                     </li>
                  </ul>
                  {{/compare}}
               </div>
            </div>
            <div class="blog-comment mt-5">
               <h4 class="mb-5">1 Comment</h4>
               <div
                  class="comment-wrap d-flex p-5 border position-relative flex-row flex-column flex-lg-row flex-md-row gap-4">
                  <div>
                     <img src="{{path}}assets/images/blog/avatar.webp"
                        alt="author"
                        class="img-fluid">
                  </div>
                  <div class="comments-info">
                     <h5>Admin</h5>
                     <p class="my-2">Mind Blowing</p>
                     <div class="list-inline-item">
                        <i aria-hidden="true"
                           class="far fa-calendar-alt me-1 text-primary small"></i>
                        <a href=" " class="text-uppercase small">March 24,
                           2022</a>
                     </div>
                     <div class="button-secondary">
                        {{> components/widgets/button button-url="#"
                        button-text="Reply"}}
                     </div>
                  </div>
               </div>
               <h4 class="my-5">Leave A Reply</h4>
               <p>Your email address will not be published. Required fields are
                  marked *</p>
               <form method="post" class="mt-5">
                  <div class="custom-form-field">
                     <textarea id="comment" name="comment" placeholder="Comment"
                        required="required"
                        class="form-control"></textarea>
                  </div>
                  <div class="row my-5">
                     <div class="col-lg-4 col-md-6 my-5 my-lg-0">
                        <div class="custom-form-field">
                           <input id="author" name="author" aria-required="true"
                              required="required"
                              placeholder="Name*" class="form-control">
                        </div>
                     </div>
                     <div class="col-lg-4 col-md-6 my-5 my-lg-0">
                        <div class="custom-form-field">
                           <input id="email" name="email" required="required"
                              placeholder="Email*"
                              class="form-control">
                        </div>
                     </div>
                     <div class="col-lg-4 col-md-6 my-5 my-lg-0">
                        <div class="custom-form-field">
                           <input id="url" name="url" placeholder="Website"
                              class="form-control">
                        </div>
                     </div>
                  </div>
                  <label class="mb-5 custom-form-field">
                     <input type="checkbox" required="required"> <span
                        class="checkmark"></span><span>Save my
                        name, email, and website in this browser for the next
                        time I comment.</span>
                  </label>
                  <div class="button-secondary mb-3">
                     {{> components/widgets/button button-url="#"
                     button-text="Post Comment"}}
                  </div>
               </form>
            </div>
