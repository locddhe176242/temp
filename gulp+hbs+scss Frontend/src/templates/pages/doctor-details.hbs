{{#extend 'layouts/default' title="Doctor Details" isHeader="true" isBreadcrumb="true" isFooter="true"
active="doctor-details" path="./" isSwiperSlider="true"}}
{{#content "content"}}
<div class="section-padding team-detail">
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <div class="team-member-info">
                    {{> components/widgets/team-overdetail team-image="/team/team-1.webp"
                    team-member-name="Gunner Peha" team-specialized="Cardiologist"}}
                </div>
                <div class="mt-5 sidebar-widget-working-hour bg-secondary-subtle text-body p-3 p-md-5">
                    <div class="icon mb-4">
                        <i class="far fa-clock"></i>
                    </div>
                    <h4 class="mb-3 fw-500">Opening Time</h4>
                    <ul class="list-inline m-0">
                        <li class="mb-2 pb-2 border-bottom d-flex align-items-center justify-content-between">
                            <span>Monday - Friday</span>
                            <span>6:00 - 7:00 pm</span>
                        </li>
                        <li class="mb-2 pb-2 border-bottom d-flex align-items-center justify-content-between">
                            <span>Saturday</span>
                            <span>8:00 - 9:00 pm </span>
                        </li>
                        <li class="d-flex align-items-center justify-content-between">
                            <span>Sunday</span>
                            <span>Closed</span>
                        </li>
                    </ul>
                </div>
                <div class="mt-5 sidebar-widget-services bg-primary-subtle text-body p-3 p-md-5">
                    <div class="image mb-5">
                        <img src="{{path}}assets/images/pages/short-logo.png" class="img-fluid" alt="image">
                    </div>
                    <h4 class="mb-3">KiviCare Services</h4>
                    <ul class="list-inline m-0">
                        <li class="mb-2">
                            <span><b>Call :</b></span>
                            <span>+123456789</span>
                        </li>
                        <li class="mb-2">
                            <span><b>Mail :</b></span>
                            <span>support@example.com</span>
                        </li>
                        <li>
                            <span><b>Address :</b></span>
                            <span>1234, North Avenue, Luke Lane, South Bend, IN 360001</span>
                        </li>
                    </ul>
                    <div class="sidebar-widget-social-list mt-5">
                        <ul class="list-inline m-0 p-0 d-flex align-items-center gap-3 flex-wrap">
                            <li>
                                <a href="https://www.facebook.com/"
                                    class="facebook-icon d-inline-block text-white text-center">
                                    <i class="fab fa-facebook"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://x.com/"
                                    class="twitter-icon d-inline-block text-white text-center">
                                    <i class="fab fa-twitter"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.youtube.com/"
                                    class="youtube-icon d-inline-block text-white text-center">
                                    <i class="fab fa-youtube"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.linkedin.com/"
                                    class="linkedin-icon d-inline-block text-white text-center">
                                    <i class="fab fa-linkedin"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-lg-8 mt-lg-0 mt-5">
                <div class="iq-title-box">
                    <h3 class="iq-title iq-heading-title mb-3">
                        <span class="right-text text-capitalize">General</span>
                        <span class="left-text text-capitalize fw-light">Surgeon</span>
                    </h3>
                    <p class="iq-title-desc text-body">
                        Dr. Mantooth has done Basic and Advanced training in Reproductive Medicine and IVF- Homerton
                        University hospital London UK. He is an active member of London Fertility Society and has helped
                        in conducting workshops in IFS conference.
                    </p>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <h5 class="fw-normal">Honors and Awards </h5>
                        <div class="row mt-5 g-3">
                            <div class="col-sm-6 mt-0">
                                <div class="px-3 pt-2 pb-3 border">
                                    <div class="text-primary">
                                        <i aria-hidden="true" class="flaticon flaticon-flaticon flaticon-award"></i>
                                    </div>
                                    <h6 class="mt-1 mb-0">2020 World Health Organization Vaccine Research Award</h6>
                                </div>
                            </div>
                            <div class="col-sm-6 mt-4 mt-sm-0">
                                <div class="px-3 pt-2 pb-3 border">
                                    <div class="text-primary">
                                        <i aria-hidden="true" class="flaticon flaticon-flaticon flaticon-award"></i>
                                    </div>
                                    <h6 class="mt-1 mb-0">2016 Best Skincare Treatment Winning Trophy & Recognition</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 mt-md-0 mt-5">
                        <h5 class="fw-normal">Skillset</h5>
                        <div class="mt-5">
                            {{> components/widgets/progressbar progress-title="Technique"
                            progress-value="70"}}
                        </div>
                        <div class="mt-5">
                            {{> components/widgets/progressbar progress-title="Empathy"
                            progress-value="80"}}
                        </div>
                        <div class="mt-5">
                            {{> components/widgets/progressbar progress-title="Operation"
                            progress-value="90"}}
                        </div>
                    </div>
                </div>
                <div class="mt-5 px-5 py-3 border">
                    <div class="row align-items-center">
                        <div class="col-md-2">
                            <h5 class="m-0">Specialty</h5>
                        </div>
                        <div class="col-md-10 mt-md-0 mt-2">
                            <span class="text-primary"><i class="far fa-star"></i></span>
                            <span class="ms-1">Gynecologist</span>
                        </div>
                    </div>
                </div>
                <div class="mt-3 px-5 py-3 border">
                    <div class="row align-items-center">
                        <div class="col-md-2">
                            <h5 class="m-0">Education</h5>
                        </div>
                        <div class="col-md-10 mt-md-0 mt-2">
                            <ul class="list-inline m-0">
                                <li class="mb-2">
                                    <span class="text-primary"><i class="fas fa-check"></i></span>
                                    <span class="ms-1">Embryology</span>
                                </li>
                                <li class="mb-2">
                                    <span class="text-primary"><i class="fas fa-check"></i></span>
                                    <span class="ms-1">Doctor of Medicine, University of texas, USA (1990)</span>
                                </li>
                                <li>
                                    <span class="text-primary"><i class="fas fa-check"></i></span>
                                    <span class="ms-1">Medical Orientation Program, St. Louis University (St. Louis,
                                        Missouri
                                        1996)</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="mt-3 px-5 py-3 border">
                    <div class="row align-items-center">
                        <div class="col-md-2">
                            <h5 class="m-0">Experience</h5>
                        </div>
                        <div class="col-md-10 mt-md-0 mt-2">
                            <span class="text-primary"><i class="fas fa-file-alt"></i></span>
                            <span class="ms-1">15 Years of experience in medicine</span>
                        </div>
                    </div>
                </div>
                <div class="mt-5">
                    <div class="iq-title-box mb-0">
                        <span class="iq-subtitle">DOCTORS</span>
                        <h4 class="iq-title iq-heading-title mb-0">
                            <span class="right-text text-capitalize">Recommended</span>
                            <span class="left-text text-capitalize fw-light">Colleague Doctors</span>
                        </h4>
                    </div>
                </div>
                <div class="mt-4">
                    <div class="position-relative overflow-hidden swiper swiper-general" data-slide="2" data-laptop="2"
                        data-tab="2" data-mobile="1" data-mobile-sm="1" data-autoplay="true" data-loop="true"
                        data-navigation="true" data-pagination="true">
                        <div class="swiper-wrapper">
                            <div class="swiper-slide">
                                {{> components/widgets/team-overdetail
                                team-image="/team/team-1.webp" team-member-name="Gunner Peha"
                                team-specialized="Cardiologist"}}
                            </div>
                            <div class="swiper-slide">
                                {{> components/widgets/team-overdetail
                                team-image="/team/team-2.webp" team-member-name="Ayden Leabow" team-specialized="General
                                Surgeon"}}
                            </div>
                            <div class="swiper-slide">
                                {{> components/widgets/team-overdetail
                                team-image="/team/team-3.webp" team-member-name="Jennifer Dumont"
                                team-specialized="General Surgeon"}}
                            </div>
                            <div class="swiper-slide">
                                {{> components/widgets/team-overdetail
                                team-image="/team/team-4.webp" team-member-name="Damian Melcher"
                                team-specialized="Gynecologist"}}
                            </div>
                            <div class="swiper-slide">
                                {{> components/widgets/team-overdetail
                                team-image="/team/team-5.webp" team-member-name="Lewis Scobee"
                                team-specialized="Cardiologist"}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{/content}}
{{/extend}}