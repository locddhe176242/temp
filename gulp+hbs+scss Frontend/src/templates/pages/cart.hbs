{{#extend 'layouts/default' isSweetalert="true" title="Cart" isHeader="true" isBreadcrumb="true" 
isFooter="true"
active="cart" path="./" }}
{{#content "content"}}
<div class="cart-page  section-padding">
   <div class="container">
      <div class="main-cart mb-3 mb-md-5 pb-0 pb-md-5">
         <ul class="cart-page-items d-flex justify-content-center list-inline align-items-center gap-3 gap-md-5 flex-wrap">
            <li class="cart-page-item active">
               <span class="cart-pre-heading badge cart-pre-number bg-primary border-radius rounded-circle me-1"> 1 </span>
               <span class="cart-page-link ">
               Shopping Cart </span>
            </li>
            <li>
               <svg width="25" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd"
                     d="M12 21.2498C17.108 21.2498 21.25 17.1088 21.25 11.9998C21.25 6.89176 17.108 2.74976 12 2.74976C6.892 2.74976 2.75 6.89176 2.75 11.9998C2.75 17.1088 6.892 21.2498 12 21.2498Z"
                     stroke="currentColor" stroke-width="1.5" stroke-linecap="round"
                     stroke-linejoin="round"></path>
                  <path d="M10.5576 15.4709L14.0436 11.9999L10.5576 8.52895" stroke="currentColor"
                     stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
               </svg>
            </li>
            <li>
               <span class=" cart-pre-number bg-light border-radius rounded-circle me-1">
               2 </span>
               <span class="cart-page-link ">
               Checkout </span>
            </li>
            <li>
               <svg width="25" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" clip-rule="evenodd"
                     d="M12 21.2498C17.108 21.2498 21.25 17.1088 21.25 11.9998C21.25 6.89176 17.108 2.74976 12 2.74976C6.892 2.74976 2.75 6.89176 2.75 11.9998C2.75 17.1088 6.892 21.2498 12 21.2498Z"
                     stroke="currentColor" stroke-width="1.5" stroke-linecap="round"
                     stroke-linejoin="round"></path>
                  <path d="M10.5576 15.4709L14.0436 11.9999L10.5576 8.52895" stroke="currentColor"
                     stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
               </svg>
            </li>
            <li>
               <span class=" cart-pre-number bg-light border-radius rounded-circle me-1"> 3 </span>
               <span class="cart-page-link ">
               Order Summary </span>
            </li>
         </ul>
      </div>
      <div class="table-responsive">
         <table class="table cart-table">
            <thead class="border-bottom">
               <tr>
                  <th scope="col" class="fw-bold fs-5 text-primary"></th>
                  <th scope="col" class="fw-bold fs-5 text-primary">Product</th>
                  <th scope="col" class="fw-bold fs-5 text-primary">Price</th>
                  <th scope="col" class="fw-bold fs-5 text-primary">Quantity</th>
                  <th scope="col" class="fw-bold fs-5 text-primary">Subtotal</th>
               </tr>
            </thead>
            <tbody>
               <tr data-item="list">
                  <td>
                     <button
                        class="btn btn-icon btn-danger btn-sm delete-btn bg-transparent text-primary border-0">
                     <span class="btn-inner">
                     <i class="fas fa-times"></i>
                     </span>
                     </button>
                  </td>
                  <td>
                     <div class="product-thumbnail">
                        <a class="d-block mb-2" href="">
                        <img class="avatar-80"
                           src="{{path}}assets/images/shop/product-1.webp" alt="">
                        </a>
                        <span class="mt-2 text-primary">Electric Toothbrush</span>
                     </div>
                  </td>
                  <td>
                     <span class="fs-5 fw-500">$123</span>
                  </td>
                  <td>
                     <div class="btn-group iq-qty-btn" data-qty="btn" role="group">
                        <button type="button" class="btn btn-sm btn-outline-light iq-quantity-minus text-dark">
                           <svg xmlns="http://www.w3.org/2000/svg" width="6" height="3" viewBox="0 0 6 3"
                              fill="none">
                              <path d="M5.22727 0.886364H0.136364V2.13636H5.22727V0.886364Z"
                                 fill="currentColor"></path>
                           </svg>
                        </button>
                        <input type="text" class="btn btn-sm btn-outline-light input-display"
                           data-qty="input" pattern="^(0|[1-9][0-9]*)$" minlength="1" maxlength="2"
                           value="2" title="Qty" >
                        <button type="button" class="btn btn-sm btn-outline-light iq-quantity-plus text-dark">
                           <svg xmlns="http://www.w3.org/2000/svg" width="9" height="8" viewBox="0 0 9 8"
                              fill="none">
                              <path
                                 d="M3.63636 7.70455H4.90909V4.59091H8.02273V3.31818H4.90909V0.204545H3.63636V3.31818H0.522727V4.59091H3.63636V7.70455Z"
                                 fill="currentColor"></path>
                           </svg>
                        </button>
                     </div>
                  </td>
                  <td>
                     <span class="sub-total fs-5 fw-500">$123</span>
                  </td>
               </tr>
               <tr data-item="list">
                  <td>
                     <button
                        class="btn btn-icon btn-danger btn-sm delete-btn bg-transparent text-primary border-0">
                     <span class="btn-inner">
                     <i class="fas fa-times"></i>
                     </span>
                     </button>
                  </td>
                  <td>
                     <div class="product-thumbnail">
                        <a class="d-block mb-2" href="">
                        <img class="avatar-80"
                           src="{{path}}assets/images/shop/product-2.webp" alt="">
                        </a>
                        <span class="mt-2 text-primary">Medical Box</span>
                     </div>
                  </td>
                  <td>
                     <span class="fs-5 fw-500">$92</span>
                  </td>
                  <td>
                     <div class="btn-group iq-qty-btn" data-qty="btn" role="group">
                        <button type="button" class="btn btn-sm btn-outline-light iq-quantity-minus text-dark">
                           <svg xmlns="http://www.w3.org/2000/svg" width="6" height="3" viewBox="0 0 6 3"
                              fill="none">
                              <path d="M5.22727 0.886364H0.136364V2.13636H5.22727V0.886364Z"
                                 fill="currentColor"></path>
                           </svg>
                        </button>
                        <input type="text" class="btn btn-sm btn-outline-light input-display"
                           data-qty="input" pattern="^(0|[1-9][0-9]*)$" minlength="1" maxlength="2"
                           value="2" title="Qty" >
                        <button type="button" class="btn btn-sm btn-outline-light iq-quantity-plus text-dark">
                           <svg xmlns="http://www.w3.org/2000/svg" width="9" height="8" viewBox="0 0 9 8"
                              fill="none">
                              <path
                                 d="M3.63636 7.70455H4.90909V4.59091H8.02273V3.31818H4.90909V0.204545H3.63636V3.31818H0.522727V4.59091H3.63636V7.70455Z"
                                 fill="currentColor"></path>
                           </svg>
                        </button>
                     </div>
                  </td>
                  <td>
                     <span class="sub-total fs-5 fw-500">$92</span>
                  </td>
               </tr>
               <tr data-item="list">
                  <td>
                     <button
                        class="btn btn-icon btn-danger btn-sm delete-btn bg-transparent text-primary border-0">
                     <span class="btn-inner">
                     <i class="fas fa-times"></i>
                     </span>
                     </button>
                  </td>
                  <td>
                     <div class="product-thumbnail">
                        <a class="d-block mb-2" href="">
                        <img class="avatar-80"
                           src="{{path}}assets/images/shop/product-3.webp" alt="">
                        </a>
                        <span class="mt-2 text-primary">Hand Sanitizer Bottle</span>
                     </div>
                  </td>
                  <td>
                     <span class="fs-5 fw-500">$90</span>
                  </td>
                  <td>
                     <div class="btn-group iq-qty-btn" data-qty="btn" role="group">
                        <button type="button" class="btn btn-sm btn-outline-light iq-quantity-minus text-dark">
                           <svg xmlns="http://www.w3.org/2000/svg" width="6" height="3" viewBox="0 0 6 3"
                              fill="none">
                              <path d="M5.22727 0.886364H0.136364V2.13636H5.22727V0.886364Z"
                                 fill="currentColor"></path>
                           </svg>
                        </button>
                        <input type="text" class="btn btn-sm btn-outline-light input-display"
                           data-qty="input" pattern="^(0|[1-9][0-9]*)$" minlength="1" maxlength="2"
                           value="2" title="Qty"  >
                        <button type="button" class="btn btn-sm btn-outline-light iq-quantity-plus text-dark">
                           <svg xmlns="http://www.w3.org/2000/svg" width="9" height="8" viewBox="0 0 9 8"
                              fill="none">
                              <path
                                 d="M3.63636 7.70455H4.90909V4.59091H8.02273V3.31818H4.90909V0.204545H3.63636V3.31818H0.522727V4.59091H3.63636V7.70455Z"
                                 fill="currentColor"></path>
                           </svg>
                        </button>
                     </div>
                  </td>
                  <td>
                     <span class="sub-total fs-5 fw-500">$90</span>
                  </td>
               </tr>
            </tbody>
         </table>
      </div>
      <div class="coupon-main d-flex justify-content-between  gap-5 flex-wrap align-items-center pt-4 pb-5 border-bottom">
         <div class="wrap-coupon d-flex align-items-center gap-3 flex-wrap">
            <label>Coupon :</label>
            <input class="form-control d-inline-block w-auto me-2" name="coupon_code" type="text"
               placeholder="Coupon code">
            <div class=" d-inline-block">
               {{> components/widgets/button button-text="Apply Coupon" }}
            </div>
         </div>
         <div class="button-primary">
            {{> components/widgets/button button-text="Update Cart" }}
         </div>
      </div>
      <div class="cart_totals p-5 bg-primary-subtle">
         <h5 class="mb-3">Cart Totals</h5>
         <div class="css_prefix-woocommerce-cart-box table-responsive">
            <table class="table  border">
               <tbody>
                  <tr class="cart-subtotal">
                     <th class="border-0"><span class="text-dark fw-normal">Subtotal</span></th>
                     <td class="border-0">
                        <span class="fs-5 text-primary">$305</span>
                     </td>
                  </tr>
                  <tr class="order-total">
                     <th class="border-0"><strong>
                        <span class="fs-5 text-dark"> Total </span>      
                        </strong> 
                     </th>
                     <td class="border-0">
                        <span class="fs-5 text-primary">$305</span>
                     </td>
                  </tr>
               </tbody>
            </table>
            <div class="button-primary">
               {{> components/widgets/button button-url="checkout.html" button-text="Proceed to checkout" }}
            </div>
         </div>
      </div>
   </div>
</div>
{{/content}}
{{/extend}}